<!DOCTYPE html>
<html lang="en">
   <%- include('../partial-views/header') %>
<body>
    <h1>Student Details</h1>
<table>
    <tr>
        <th>Name</th>
        <th>Height</th>
    </tr>
        <% students.forEach((student) => { %>
            <tr>
                <td><%= student.name %></td>
               <td><%= student.height %></td>
            </tr>
        <% }) %>
</table>
</body>
</html>